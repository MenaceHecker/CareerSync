{
    "manifest_version": 3,
    "name": "CareerSync",
    "version": "1.0",
    "description": "AI-powered resume vs job description matcher.",
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "run_at": "document_idle"
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  